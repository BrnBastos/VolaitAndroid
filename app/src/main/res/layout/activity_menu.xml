<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgray"
    tools:context=".MenuActivity">


    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="566dp"
        android:layout_height="70dp"
        android:layout_marginTop="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.561"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/menu__1_" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="58dp"
        android:layout_height="26dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:srcCompat="@drawable/volaitmain" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="559dp"
        android:layout_height="78dp"
        android:layout_marginTop="672dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/menu__1_" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="54dp"
        android:layout_height="55dp"
        android:backgroundTint="#FFFFFF"
        android:onClick="Telahome"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView8"
        app:srcCompat="@drawable/home" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="54dp"
        android:layout_height="55dp"
        android:layout_marginBottom="1dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.32"
        app:layout_constraintStart_toEndOf="@+id/imageButton2"
        app:layout_constraintTop_toTopOf="@+id/imageView8"
        app:layout_constraintVertical_bias="0.666"
        app:srcCompat="@drawable/favoritos" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="54dp"
        android:layout_height="55dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView8"
        app:srcCompat="@drawable/perfil__1_" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="54dp"
        android:layout_height="55dp"
        android:backgroundTint="#FFFFFF"
        android:onClick="TelaCupom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.181"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        app:layout_constraintTop_toTopOf="@+id/imageView8"
        app:srcCompat="@drawable/cupons" />

    <LinearLayout
        android:layout_width="395dp"
        android:layout_height="601dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="2dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:layout_gravity="center"
            android:layout_marginTop="4dp"
            android:background="@color/white"
            android:gravity="center"
            android:onClick="Telahome"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/home_icon" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="190dp"
                android:layout_height="32dp"
                android:layout_weight="1"
                android:fontFamily="@font/inter_semibold"
                android:gravity="center"
                android:text="Home"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="1dp"
                android:layout_height="22dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/triangulo" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:layout_gravity="center"
            android:layout_marginTop="4dp"
            android:background="@color/white"
            android:gravity="center"
            android:onClick="TelaCupom"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/cupom_icon" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="190dp"
                android:layout_height="32dp"
                android:layout_weight="1"
                android:fontFamily="@font/inter_semibold"
                android:gravity="center"
                android:text="Cupom"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="1dp"
                android:layout_height="22dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/triangulo" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:layout_gravity="center"
            android:layout_marginTop="4dp"
            android:background="@color/white"
            android:gravity="center"
            android:onClick="TelaeditPerfil"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/perfil_icon" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="190dp"
                android:layout_height="32dp"
                android:layout_weight="1"
                android:fontFamily="@font/inter_semibold"
                android:gravity="center"
                android:text="Perfil"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="1dp"
                android:layout_height="22dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/triangulo" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:layout_gravity="center"
            android:layout_marginTop="4dp"
            android:background="@color/white"
            android:gravity="center"
            android:onClick="Telaprivacidade"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/privacidade_icon" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="190dp"
                android:layout_height="32dp"
                android:layout_weight="1"
                android:fontFamily="@font/inter_semibold"
                android:gravity="center"
                android:text="Privacidade"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="1dp"
                android:layout_height="22dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/triangulo" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:layout_gravity="center"
            android:layout_marginTop="4dp"
            android:background="@color/white"
            android:gravity="center"
            android:onClick="TelaSobre"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/info_icon" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="190dp"
                android:layout_height="32dp"
                android:layout_weight="1"
                android:fontFamily="@font/inter_semibold"
                android:gravity="center"
                android:text="Sobre o app"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="1dp"
                android:layout_height="22dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/triangulo" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:layout_gravity="center"
            android:layout_marginTop="4dp"
            android:background="@color/white"
            android:gravity="center"
            android:onClick="Telahome"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/sair_icon" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="190dp"
                android:layout_height="32dp"
                android:layout_weight="1"
                android:fontFamily="@font/inter_semibold"
                android:gravity="center"
                android:text="Sair da conta"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/imageView27"
                android:layout_width="1dp"
                android:layout_height="22dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/triangulo" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>